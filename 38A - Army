#include <bits/stdc++.h>
using namespace std;

int main() {
    int arr[101];
    int n;
    
    cin >> n;

    // Input array values
    for (int i = 0; i < n - 1; i++) {
        cin >> arr[i]; 
    }

    int a, b;
    cin >> a >> b;

    int result = 0;

    // Calculate the sum of elements between indices a and b (1-based index)
    for (int i = a - 1; i < b - 1; i++) {
        result += arr[i];
    }

    // Output the result
    cout << result;
    
    return 0;
}
