
#include <bits/stdc++.h>
using namespace std;
 
int main() {
string s,s2,s3,s4;
cin>>s>>s2>>s3;
s4+=s+s2;
sort(s4.begin(),s4.end());
sort(s3.begin(),s3.end());
if(s3==s4){
	cout<<"YES";
}
else{
	cout<<"NO";
}
 
 	return 0;
}
//or

#include <bits/stdc++.h>
using namespace std;
 
bool checkLetter(string s, string w, string newL)
{
    sort(newL.begin(), newL.end());
 
    string sum = s + w;
    sort(sum.begin(), sum.end());
 
    return sum == newL;
}
 
int main()
{
    string s1, s2, s3;
    cin >> s1 >> s2 >> s3;
 
    if (checkLetter(s1, s2, s3))
        cout << "YES" << endl;
    else
        cout << "NO" << endl;
 
    return 0;
}
