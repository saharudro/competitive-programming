#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n, k, l, c, d, p, nl, np;
    cin >> n >> k >> l >> c >> d >> p >> nl >> np;
 
    int total_drink = k * l;
    int drink_toasts = total_drink / nl;
    int lime_toasts = c * d;
    int salt_toasts = p / np;
    int total_toasts = min({drink_toasts, lime_toasts, salt_toasts});
 
    int toasts_per_friend = total_toasts / n;
 
    cout << toasts_per_friend << endl;
    return 0;
}
