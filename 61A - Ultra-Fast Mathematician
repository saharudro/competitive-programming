#include <bits/stdc++.h>
using namespace std;

int main() {
    string s1, s2;
    cin >> s1 >> s2;  // Input two strings s1 and s2
    
    // Loop through both strings and compare characters at the same position
    for (int i = 0; i < s1.size(); i++) {
        // If characters are the same at the current position, output 0
        if (s1[i] == s2[i]) {
            cout << 0;
        } 
        // If characters are different at the current position, output 1
        else {
            cout << 1;
        }
    }

    return 0;
}
